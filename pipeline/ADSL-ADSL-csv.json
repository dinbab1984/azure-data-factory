{
	"name": "ADSL-ADSL-csv",
	"properties": {
		"activities": [
			{
				"name": "ADSL-ADSL-csv",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "source//customers.csv"
					},
					{
						"name": "Destination",
						"value": "target//Customers.json"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Customers_csv",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Customers_json",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}